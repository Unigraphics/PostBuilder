      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      [$mom_sys_leader(Z)]
      TRAILER     [$mom_sys_trailer(Z)]
  }

  ADDRESS Z_home
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "Z"
  }

  ADDRESS fourth_axis
  {
      FORMAT      Rotary
      FORCE       off
      MAX         99999.9999 Abort
      MIN         -99999.9999 Abort
      LEADER      [$mom_sys_leader(fourth_axis)]
      TRAILER     [$mom_sys_trailer(fourth_axis)]
  }

  ADDRESS fifth_axis
  {
      FORMAT      Rotary
      FORCE       off
      MAX         99999.9999 Abort
      MIN         -99999.9999 Abort
      LEADER      [$mom_sys_leader(fifth_axis)]
      TRAILER     [$mom_sys_trailer(fifth_axis)]
  }

  ADDRESS I
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "I"
  }

  ADDRESS I_rot
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "I"
  }

  ADDRESS X_vector
  {
      FORMAT      Vector
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "I"
      ZERO_FORMAT Zero_real
  }

  ADDRESS J
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "J"
  }

  ADDRESS J_rot
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "J"
  }

  ADDRESS Y_vector
  {
      FORMAT      Vector
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "J"
      ZERO_FORMAT Zero_real
  }

  ADDRESS K
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "K"
  }

  ADDRESS K_rot
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "K"
  }

  ADDRESS helix_turn
  {
      FORMAT      Digit_3
      FORCE       always
      LEADER      "L"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Z_vector
  {
      FORMAT      Vector
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "K"
      ZERO_FORMAT Zero_real
  }

  ADDRESS R
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "R"
  }

  ADDRESS R_rot
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "R"
  }

  ADDRESS F
  {
      FORMAT      Feed
      FORCE       off
      MAX         999999.9 Warning
      MIN         .0001 Warning
      LEADER      "F"
      ZERO_FORMAT Zero_real
  }

  ADDRESS S
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         99999 Truncate
      MIN         0 Truncate
      LEADER      "S"
      ZERO_FORMAT Zero_int
  }

  ADDRESS S_max
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         99999 Truncate
      MIN         0 Truncate
      LEADER      "S"
      ZERO_FORMAT Zero_int
  }

  ADDRESS S_preset
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         99999 Truncate
      MIN         0 Truncate
      LEADER      "S"
      ZERO_FORMAT Zero_int
  }

  ADDRESS T
  {
      FORMAT      Tcode
      FORCE       off
      LEADER      "T"
  }

  ADDRESS D
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "D"
      ZERO_FORMAT Zero_int
  }

  ADDRESS H
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "H"
      ZERO_FORMAT Zero_int
  }

  ADDRESS dwell
  {
      FORMAT      Dwell_SECONDS
      FORCE       always
      MAX         99999.999 Truncate
      MIN         0.000 Truncate
      LEADER      "X"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_dwell
  {
      FORMAT      Dwell_MILLSECONDS
      FORCE       off
      LEADER      "P"
      ZERO_FORMAT Zero_int
  }

  ADDRESS K_cycle
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "K"
  }

  ADDRESS Q_cycle
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "Q"
  }

  ADDRESS R_cycle
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      "R"
  }

  ADDRESS M_spindle
  {
      FORMAT      Digit_command
      FORCE       off
      LEADER      [$mom_sys_leader(M_spindle)]
      TRAILER     [$mom_sys_trailer(M_spindle)]
      ZERO_FORMAT Zero_command
  }

  ADDRESS M_range
  {
      FORMAT      Digit_command
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_command
  }

  ADDRESS M_coolant
  {
      FORMAT      Digit_command
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_command
  }

  ADDRESS M_clamp_fourth
