      FORMAT      Digit_command
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_command
  }

  ADDRESS G_motion
  {
      FORMAT      Digit_command
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_command
  }

  ADDRESS G_return
  {
      FORMAT      Digit_command
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_command
  }

  ADDRESS Text
  {
      FORMAT      String
      FORCE       always
      LEADER      ""
  }

  ADDRESS G_mode
  {
      FORMAT      Digit_command
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_command
  }

  ADDRESS G_dwell
  {
      FORMAT      Digit_command
      FORCE       always
      LEADER      "G"
      ZERO_FORMAT Zero_command
  }

  ADDRESS G
  {
      FORMAT      Digit_3
      FORCE       off
      LEADER      [$mom_sys_leader(G)]
      TRAILER     [$mom_sys_trailer(G)]
      ZERO_FORMAT Zero_int
  }

  ADDRESS X
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
      LEADER      [$mom_sys_leader(X)]
      TRAILER     [$mom_sys_trailer(X)]
  }

  ADDRESS X_home
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "X"
  }

  ADDRESS Y
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Abort
      MIN         -99999.999 Abort
