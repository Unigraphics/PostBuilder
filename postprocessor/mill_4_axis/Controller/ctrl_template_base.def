  {
      FORMAT      Digit_command
      FORCE       always
      LEADER      [$::mom_sys_leader(M)]
      TRAILER     [$::mom_sys_trailer(M)]
      ZERO_FORMAT Zero_command
  }

  ADDRESS Pid
  {
      FORMAT      Digit04
      FORCE       always
      MAX         9999 Truncate
      MIN         0 Truncate
      LEADER      "O"
  }

  ADDRESS turn_cycle_seqno_begin
  {
      FORMAT      Digit_8
      FORCE       always
      LEADER      "P"
  }

  ADDRESS turn_cycle_seqno_end
  {
      FORMAT      Digit_8
      FORCE       always
      LEADER      "Q"
  }

  ADDRESS turn_cycle_feed
  {
      FORMAT      Coordinate
      FORCE       always
      LEADER      "F"
      ZERO_FORMAT Zero_real
  }

  ADDRESS turn_cycle_depth
  {
      FORMAT      Coordinate
      FORCE       always
      LEADER      "U"
      ZERO_FORMAT Zero_real
  }

  ADDRESS turn_cycle_retract
  {
      FORMAT      Coordinate
      FORCE       always
      LEADER      "R"
      ZERO_FORMAT Zero_real
  }

  ADDRESS turn_cycle_msg
  {
      FORMAT      String
      FORCE       off
      LEADER      "("
      TRAILER     ")"
  }

  ADDRESS turn_cycle_feed
  {
      FORMAT      Coordinate
      FORCE       always
      LEADER      "F"
      ZERO_FORMAT Zero_real
  }

  ADDRESS turn_cycle_speed
  {
      FORMAT      Digit_5
      FORCE       always
      LEADER      "S"
      ZERO_FORMAT Zero_int
  }

  ADDRESS turn_cycle_stock_x
  {
      FORMAT      Coordinate
      FORCE       always
      LEADER      "U"
      ZERO_FORMAT Zero_real
  }

  ADDRESS turn_cycle_stock_z
  {
      FORMAT      Coordinate
      FORCE       always
      LEADER      "W"
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_AAXIS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_ENUM
  {
      FORMAT      Digit_5
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_BAXIS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_XABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_FEED
  {
      FORMAT      Feed
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_YABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_SPEED
  {
      FORMAT      Rev
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_ZABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

############ BLOCK TEMPLATE DECLARATIONS #############
  BLOCK_TEMPLATE absolute_mode
  {
       G_cutcom[$mom_sys_cutcom_code(OFF)]
       G_plane[$mom_sys_cutcom_plane_code(XY)]\opt
       G_mode[$mom_sys_output_code(ABSOLUTE)]
       G[$mom_sys_unit_code($mom_output_unit)]\opt
  }

  BLOCK_TEMPLATE auxfun
  {
       M[$mom_auxfun]
  }

  BLOCK_TEMPLATE channel_init
  {
       G_cutcom[40]
       G_zero[99]
       G_plane[99]
       G_feed[99]
       G_spin[99]
       G_spin_max[99]
       G_motion[99]
       G_mode[99]
       X[99999.999]
       Y[99999.999]
       Z[99999.999]
       fourth_axis[9999.999]
       fifth_axis[9999.999]
       I[99999.999]
       J[99999.999]
       K[99999.999]
       R[99999.999]
       D[0]
       H[99]
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE circular_move
  {
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_adjust[$mom_sys_adjust_code]
       G_feed[$mom_sys_feed_rate_mode_code($feed_mode)]
       G_motion[$mom_sys_circle_code($mom_arc_direction)]\opt
       G_mode[$mom_sys_output_code($mom_output_mode)]
       X[$mom_pos(0)*$x_factor]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       fourth_axis[$mom_out_angle_pos(0)]
       fifth_axis[$mom_out_angle_pos(1)]
       I[$mom_pos_arc_center(0) - $mom_prev_pos(0)]
       J[$mom_pos_arc_center(1) - $mom_prev_pos(1)]
       K[$mom_pos_arc_center(2) - $mom_prev_pos(2)]
       helix_turn[$mom_helix_turn_number]\opt
       R[$mom_arc_radius]
       F[$feed]
       S[$mom_spindle_rpm]
       D[$cutcom_adjust_register]\opt
       H[$mom_tool_adjust_register]
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
       M_coolant[$mom_sys_coolant_code($mom_coolant_mode)]\opt
  }

  BLOCK_TEMPLATE circular_move_turbo
  {
       G_cutcom[^sys_cutcom_code(CUTCOM_STATUS)]\opt
       G_plane[^sys_cutcom_plane_code(CUTCOM_PLANE)]\opt
       G_feed[^sys_feed_rate_mode_code(FEED_MODE)]\opt
       G_motion[^sys_circle_code(ARC_DIRECTION)]\opt
       G_mode[^sys_output_code(OUTPUT_MODE)]
       X[^POSX]
       Y[^POSY]
       Z[^POSZ]
       I[^CENTERX]
       J[^CENTERY]
       K[^CENTERZ]
       F[^FEED]
       M_spindle[^sys_spindle_direction_code(SPINDLE_DIRECTION)]\opt
       M_coolant[^sys_coolant_code(COOLNT_STATUS)]\opt
  }

  BLOCK_TEMPLATE circular_move_turn
  {
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
       G_plane[$mom_sys_cutcom_plane_code(ZX)]\opt
       G_feed[$mom_sys_feed_rate_mode_code($feed_mode)]
       G_motion[$mom_sys_circle_code($mom_arc_direction)]\opt
       G_mode[$mom_sys_output_code($mom_output_mode)]
       X[$mom_pos(0)*$x_factor]
       Z[$mom_pos(2)]
       I[$mom_pos_arc_center(0) - $mom_prev_pos(0)]
       K[$mom_pos_arc_center(2) - $mom_prev_pos(2)]
       R[$mom_arc_radius]
       F[$feed]
       S[$mom_spindle_speed]
       D[$cutcom_adjust_register]\opt
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
       M_coolant[$mom_sys_coolant_code($mom_coolant_mode)]\opt
  }

  BLOCK_TEMPLATE circular_move_rough_turn_cycle
  {
       G_motion[$mom_sys_circle_code($mom_arc_direction)]\opt
       X[$mom_pos(0)*$x_factor]
       Z[$mom_pos(2)]
       I[$mom_pos_arc_center(0) - $mom_prev_pos(0)]\opt
       K[$mom_pos_arc_center(2) - $mom_prev_pos(2)]\opt
  }

  BLOCK_TEMPLATE clamp_fifth_axis
  {
       M_clamp_fifth[$mom_sys_clamp_code_fifth]
  }

  BLOCK_TEMPLATE clamp_fourth_axis
  {
       M_clamp_fourth[$mom_sys_clamp_code_fourth]
  }

  BLOCK_TEMPLATE comment_blk
  {
       " "
  }

  BLOCK_TEMPLATE coolant_off
  {
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE coolant_on
  {
       M_coolant[$mom_sys_coolant_code($mom_coolant_mode)]
  }

  BLOCK_TEMPLATE coordinate_system
