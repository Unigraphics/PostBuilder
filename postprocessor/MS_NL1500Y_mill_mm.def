MACHINE  mill

INCLUDE {
         $UGII_CAM_USER_DEF_EVENT_DIR/ude.cdl
        }

FORMATTING
{
  WORD_SEPARATOR " "
  END_OF_LINE ""
  SEQUENCE sequence_number 10  10 1 9999

################ FORMAT DECLARATIONS #################
  FORMAT AbsCoord "&__5.3_"
  FORMAT Coordinate "&__5.3_"
  FORMAT Digit_2 "&__2_00"
  FORMAT Digit_3 "&__3_00"
  FORMAT Digit_4 "&_04_00"
  FORMAT Digit_5 "&__5_00"
  FORMAT Dwell_SECONDS "&__5.3_"
  FORMAT Dwell_sec "&__2_30"
  FORMAT EventNum "&+03_00"
  FORMAT Feed "&__7.2_"
  FORMAT Feed_DPM "&__5.2_"
  FORMAT Feed_FRN "&__5.3_"
  FORMAT Feed_INV "&__5.3_"
  FORMAT Feed_IPM "&__4.1_"
  FORMAT Feed_IPR "&__1.4_"
  FORMAT Feed_MMPM "&__5.0_"
  FORMAT Feed_MMPR "&__2.3_"
  FORMAT Hcode "&__4_00"
  FORMAT P_dwell_cycle "&__2_30"
  FORMAT Rev "&__4_00"
  FORMAT Rotary "&__5.3_"
  FORMAT String "%s"
  FORMAT Tcode "%s"
  FORMAT Zero_int "&_01_0_"
  FORMAT Zero_real "&_01.0_"
  FORMAT step "&__3_30"

################ ADDRESS DECLARATIONS ################
  ADDRESS G_cutcom
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_plane
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_adjust
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_feed
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         98 Truncate
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_spin
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         97 Truncate
      MIN         96 Truncate
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_return
  {
      FORMAT      Digit_2
      FORCE       always
      MAX         28 Truncate
      MIN         28 Truncate
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_motion
  {
      FORMAT      String
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_mode
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G
  {
      FORMAT      Digit_3
      FORCE       always
      MAX         999 Truncate
      MIN         0 Truncate
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS X
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      [$mom_sys_leader(X)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Y
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      [$mom_sys_leader(Y)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Z
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      [$mom_sys_leader(Z)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS fourth_axis
  {
      FORMAT      Rotary
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(fourth_axis)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS fifth_axis
  {
      FORMAT      Rotary
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(fifth_axis)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS I
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "I"
      ZERO_FORMAT Zero_real
  }

  ADDRESS J
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "J"
      ZERO_FORMAT Zero_real
  }

  ADDRESS K
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "K"
      ZERO_FORMAT Zero_real
  }

  ADDRESS R
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "R"
      ZERO_FORMAT Zero_real
  }

  ADDRESS F
  {
      FORMAT      Feed
      FORCE       off
      MAX         999999.9999 Truncate
      MIN         0.0001 Truncate
      LEADER      "F"
      ZERO_FORMAT Zero_real
  }

  ADDRESS tap_cycle_M329
  {
      FORMAT      AbsCoord
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_real
  }

  ADDRESS S
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         99999 Truncate
      MIN         0 Truncate
      LEADER      "S"
      ZERO_FORMAT Zero_int
  }

  ADDRESS T
  {
      FORMAT      Tcode
      FORCE       always
      MAX         9999 Truncate
      MIN         0 Truncate
      LEADER      "T"
  }

  ADDRESS D
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "D"
      TRAILER     "."
      ZERO_FORMAT Zero_int
  }

  ADDRESS H
  {
      FORMAT      Hcode
      FORCE       off
      MAX         9999 Truncate
      MIN         0 Truncate
      LEADER      "H"
      TRAILER     "."
      ZERO_FORMAT Zero_int
  }

  ADDRESS dwell
  {
      FORMAT      Dwell_sec
      FORCE       always
      MAX         99.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_int
  }

  ADDRESS cycle_dwell
  {
      FORMAT      P_dwell_cycle
      FORCE       off
      MAX         99.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_int
  }

  ADDRESS K_cycle
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "K"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_step1
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "I"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_step
  {
      FORMAT      step
      FORCE       always
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_int
  }

  ADDRESS cycle_orient
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS P_cutcom
  {
      FORMAT      Dwell_sec
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Q_cutcom
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS E
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         99999.999 Truncate
      MIN         -99999.999 Truncate
      LEADER      "E"
      ZERO_FORMAT Zero_real
  }

  ADDRESS M_spindle
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M_range
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M_coolant
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M
  {
      FORMAT      Digit_3
      FORCE       always
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS N
  {
      FORMAT      Digit_4
      FORCE       always
      MAX         9999 Truncate
      LEADER      [$mom_sys_leader(N)]
  }

  ADDRESS Uhome
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "U"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Vhome
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "V"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Whome
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "W"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Hhome
  {
      FORMAT      Digit_3
      FORCE       off
      LEADER      "H"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Text
  {
      FORMAT      String
      FORCE       always
      LEADER      ""
  }


################ ADDRESS DECLARATIONS ################
  ADDRESS LF_AAXIS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_ENUM
  {
      FORMAT      Digit_5
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_BAXIS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_XABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_FEED
  {
      FORMAT      Feed
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_YABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_SPEED
  {
      FORMAT      Rev
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_ZABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }


############ BLOCK TEMPLATE DECLARATIONS #############
  BLOCK_TEMPLATE Whome
  {
       G[$mom_sys_return_code]
       Whome[0]
  }

  BLOCK_TEMPLATE absolute_mode
  {
       G_cutcom[$mom_sys_cutcom_code(OFF)]
       G_plane[$mom_sys_cutcom_plane_code(XY)]
