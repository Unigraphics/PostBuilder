MACHINE  lathe

INCLUDE {
         $UGII_CAM_USER_DEF_EVENT_DIR/ude.cdl
        }

FORMATTING
{
  WORD_SEPARATOR " "
  END_OF_LINE ""
  SEQUENCE sequence_number 10  10 1 9999

################ FORMAT DECLARATIONS #################
  FORMAT AbsCoord "&__5.4_"
  FORMAT Coordinate "&__4.4_"
  FORMAT Digit_2 "&__2_00"
  FORMAT Digit_3 "&__3_00"
  FORMAT Digit_4 "&__4_00"
  FORMAT Digit_5 "&__5_00"
  FORMAT Dwell_INVERSE "&__3_00"
  FORMAT Dwell_REVOLUTIONS "&__2_30"
  FORMAT Dwell_SECONDS "&__2_30"
  FORMAT EventNum "&+03_00"
  FORMAT Feed "&__7.2_"
  FORMAT Feed_FRN "&__5.3_"
  FORMAT Feed_INV "&__5.3_"
  FORMAT Feed_IPM "&__4.1_"
  FORMAT Feed_IPR "&__1.4_"
  FORMAT Feed_MMPM "&__5.0_"
  FORMAT Feed_MMPR "&__2.3_"
  FORMAT Hcode "&__2_00"
  FORMAT Rev "&__4_00"
  FORMAT String "%s"
  FORMAT Tcode "%s"
  FORMAT Zero_int "&_01_0_"
  FORMAT Zero_real "&_01.0_"

################ ADDRESS DECLARATIONS ################
  ADDRESS G_cutcom
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_plane
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_adjust
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_feed
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         98 Truncate
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_spin
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_return
  {
      FORMAT      Digit_2
      FORCE       always
      MAX         28 Truncate
      MIN         28 Truncate
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_motion
  {
      FORMAT      String
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_mode
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G_dwell
  {
      FORMAT      Digit_2
      FORCE       always
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS G
  {
      FORMAT      Digit_3
      FORCE       always
      MAX         999 Truncate
      MIN         0 Truncate
      LEADER      "G"
      ZERO_FORMAT Zero_int
  }

  ADDRESS X
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(X)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Y
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(Y)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Z
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(Z)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS I
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "I"
      ZERO_FORMAT Zero_real
  }

  ADDRESS J
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "J"
      ZERO_FORMAT Zero_real
  }

  ADDRESS K
  {
      FORMAT      Coordinate
      FORCE       always
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "K"
      ZERO_FORMAT Zero_real
  }

  ADDRESS R
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "R"
      ZERO_FORMAT Zero_real
  }

  ADDRESS F
  {
      FORMAT      Feed
      FORCE       off
      MAX         9999.9 Warning
      MIN         .0001 Warning
      LEADER      "F"
      ZERO_FORMAT Zero_real
  }

  ADDRESS tap_cycle_M329
  {
      FORMAT      AbsCoord
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_real
  }

  ADDRESS S
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         99999 Truncate
      MIN         0 Truncate
      LEADER      "S"
      ZERO_FORMAT Zero_int
  }

  ADDRESS T
  {
      FORMAT      Tcode
      FORCE       always
      MAX         9999 Truncate
      MIN         0 Truncate
      LEADER      "T"
  }

  ADDRESS D
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "D"
      TRAILER     "."
      ZERO_FORMAT Zero_int
  }

  ADDRESS H
  {
      FORMAT      Hcode
      FORCE       off
      MAX         9999 Truncate
      MIN         0 Truncate
      LEADER      "H"
      ZERO_FORMAT Zero_int
  }

  ADDRESS dwell
  {
      FORMAT      Dwell_SECONDS
      FORCE       always
      MAX         99.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_int
  }

  ADDRESS cycle_dwell
  {
      FORMAT      Dwell_SECONDS
      FORCE       always
      MAX         99.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_int
  }

  ADDRESS P_cutcom
  {
      FORMAT      Dwell_SECONDS
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_int
  }

  ADDRESS K_cycle
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "K"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_step1
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "I"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_step
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS Q_cutcom
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS E
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "E"
      ZERO_FORMAT Zero_real
  }

  ADDRESS M_spindle
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M_range
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M_coolant
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS M
  {
      FORMAT      Digit_3
      FORCE       always
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }

  ADDRESS N
  {
      FORMAT      Digit_4
      FORCE       always
      MAX         9999 Truncate
      LEADER      [$mom_sys_leader(N)]
      ZERO_FORMAT Zero_int
  }

  ADDRESS thread_lead
  {
      FORMAT      Coordinate
      FORCE       off
      LEADER      "F"
      ZERO_FORMAT Zero_real
  }

  ADDRESS Hhome
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "H"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Uhome
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "U"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Vhome
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "V"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Text
  {
      FORMAT      String
      FORCE       always
      LEADER      ""
  }

  ADDRESS Whome
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "W"
      ZERO_FORMAT Zero_int
  }

  ADDRESS Cjoint
  {
      FORMAT      Digit_3
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_int
  }


################ ADDRESS DECLARATIONS ################
  ADDRESS LF_AAXIS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_ENUM
  {
      FORMAT      Digit_5
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_BAXIS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_XABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_FEED
  {
      FORMAT      Feed
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_YABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_SPEED
  {
      FORMAT      Rev
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_ZABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }


############ BLOCK TEMPLATE DECLARATIONS #############
  BLOCK_TEMPLATE absolute_mode
  {
       G_feed[$mom_sys_feed_rate_mode_code($feed_mode)]\opt
       G_mode[$mom_sys_output_code(ABSOLUTE)]
       G[$mom_sys_unit_code($mom_output_unit)]
  }

  BLOCK_TEMPLATE auto_tool_change
  {
       G_motion[$mom_sys_rapid_code]
       T[$lower_tcode]
  }

  BLOCK_TEMPLATE auxfun
  {
       M[$mom_auxfun]
  }

  BLOCK_TEMPLATE circular_move
  {
       G_feed[$mom_sys_feed_rate_mode_code($feed_mode)]\opt
       G_motion[$mom_sys_circle_code($mom_arc_direction)]\opt
       X[$mom_pos(0) * $mom_sys_lathe_x_factor * $mom_sys_lathe_x_double]
       Z[$mom_pos(2) * $mom_sys_lathe_z_factor]
       I[($mom_pos_arc_center(0) - $mom_prev_pos(0)) * $mom_sys_lathe_x_factor * $mom_sys_lathe_i_double]
       K[($mom_pos_arc_center(2) - $mom_prev_pos(2)) * $mom_sys_lathe_z_factor]
       F[$feed]
  }

  BLOCK_TEMPLATE coolant_off
  {
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE coolant_on
  {
       M_coolant[$mom_sys_coolant_code(ON)]\opt
  }

  BLOCK_TEMPLATE coordinate_system
  {
       G[$mom_sys_reset_code]
  }

  BLOCK_TEMPLATE cutcom_off
  {
       G_cutcom[$mom_sys_cutcom_code(OFF)]
  }

  BLOCK_TEMPLATE cutcom_on
  {
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       D[$mom_cutcom_adjust_register]\opt
  }

  BLOCK_TEMPLATE cycle_bore
  {
       G_motion[$mom_sys_cycle_bore_code]
       X[$mom_cycle_feed_to_pos(0)]
       Z[$mom_cycle_feed_to_pos(2)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]\opt
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_drill
  {
       G_motion[$mom_sys_cycle_drill_code]
       X[$mom_cycle_feed_to_pos(0)]
       Z[$mom_cycle_feed_to_pos(2)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]\opt
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_drill_break_chip
  {
       G_motion[$mom_sys_cycle_drill_break_chip_code]
       X[$mom_cycle_feed_to_pos(0)]
       Z[$mom_cycle_feed_to_pos(2)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]\opt
       F[$feed]
       cycle_step[$mom_cycle_step1]
  }

  BLOCK_TEMPLATE cycle_drill_deep
  {
       G_motion[$mom_sys_cycle_drill_deep_code]
       X[$mom_cycle_feed_to_pos(0)]
       Z[$mom_cycle_feed_to_pos(2)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]\opt
       F[$feed]
       cycle_step[$mom_cycle_step1]
  }

  BLOCK_TEMPLATE cycle_drill_dwell
  {
       G_motion[$mom_sys_cycle_drill_dwell_code]
       X[$mom_cycle_feed_to_pos(0)]
       Z[$mom_cycle_feed_to_pos(2)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]\opt
       F[$feed]
       cycle_dwell[$mom_cycle_delay]\opt
  }

  BLOCK_TEMPLATE cycle_off
  {
       G_motion[$mom_sys_cycle_off]
  }

  BLOCK_TEMPLATE cycle_tap
  {
       G_motion[$mom_sys_cycle_tap_code]
       X[$mom_cycle_feed_to_pos(0)]
       Z[$mom_cycle_feed_to_pos(2)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]\opt
       F[$feed]
  }

  BLOCK_TEMPLATE delay
  {
       G_dwell[$mom_sys_delay_code($mom_delay_mode)]
       dwell[$delay_time]
  }

  BLOCK_TEMPLATE end_of_path
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE end_of_program
  {
       M[$mom_sys_rewind_code]
  }

  BLOCK_TEMPLATE from
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_from_pos(0)]
       Z[$mom_from_pos(2)]
  }

  BLOCK_TEMPLATE home_h_axis
  {
       G[$mom_sys_return_code]
       Hhome[0]
  }

  BLOCK_TEMPLATE home_u_axis
  {
       G[$mom_sys_return_code]
       Uhome[0]
  }

  BLOCK_TEMPLATE home_uv_axis
  {
       G[$mom_sys_return_code]
       Uhome[0]
       Vhome[0]
  }

  BLOCK_TEMPLATE home_uw_axis
  {
       G[$mom_sys_return_code]
       Uhome[0]
       Whome[0]
  }

  BLOCK_TEMPLATE home_v_axis
  {
       G[$mom_sys_return_code]
       Vhome[0]
  }

  BLOCK_TEMPLATE home_w_axis
  {
       G[$mom_sys_return_code]
       Whome[0]
  }

  BLOCK_TEMPLATE inch_metric_mode
  {
       G[$mom_sys_unit_code($mom_output_unit)]
  }

  BLOCK_TEMPLATE incremental_mode
  {
       G_mode[$mom_sys_output_code(INCREMENTAL)]
  }

  BLOCK_TEMPLATE linear_move
  {
       G_feed[$mom_sys_feed_rate_mode_code($feed_mode)]\opt
       G_motion[$mom_sys_linear_code]
       X[$mom_pos(0) * $mom_sys_lathe_x_factor * $mom_sys_lathe_x_double]
       Z[$mom_pos(2) * $mom_sys_lathe_z_factor]
       F[$feed]
       M_coolant[$mom_sys_coolant_code($mom_coolant_status)]\opt
  }

  BLOCK_TEMPLATE manual_tool_change
  {
       T[$mom_tool_number]
       H[$mom_tool_adjust_register]\opt
  }

  BLOCK_TEMPLATE manual_tool_change_1
  {
       T[$mom_tool_number]
       H[$mom_tool_adjust_register]\opt
  }

  BLOCK_TEMPLATE opstop
  {
       M[$mom_sys_optional_stop_code]\opt
  }

  BLOCK_TEMPLATE output_M314_code
  {
       M[314]
  }

  BLOCK_TEMPLATE output_caxis_joint_code
  {
       Cjoint[45]
  }

  BLOCK_TEMPLATE output_caxis_release_code
  {
       M[46]
  }

  BLOCK_TEMPLATE output_cutcom_off_code
  {
       G_cutcom[$mom_sys_cutcom_code(OFF)]
  }

  BLOCK_TEMPLATE output_inch_metric_code
  {
       G[$mom_sys_unit_code($mom_kin_output_unit)]
  }

  BLOCK_TEMPLATE output_ipm_feed_code
  {
       G_feed[$mom_sys_feed_rate_mode_code(IPM)]
  }

  BLOCK_TEMPLATE output_ipr_feed_code
  {
       G_feed[$mom_sys_feed_rate_mode_code(IPR)]
  }

  BLOCK_TEMPLATE output_mcs_work_offset_code
  {
       G[$mom_fixture_offset_value + 53]
  }

  BLOCK_TEMPLATE output_rapid_G0_code
  {
       G_motion[$mom_sys_rapid_code]
  }

  BLOCK_TEMPLATE output_unit
  {
       G[$mom_sys_unit_code($mom_kin_output_unit)]
  }

  BLOCK_TEMPLATE output_work_plane_code
  {
       G[$work_plane]
  }

  BLOCK_TEMPLATE prefun
  {
       G[$mom_prefun]
  }

  BLOCK_TEMPLATE rapid_move
  {
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
       G_motion[$mom_sys_rapid_code]
       X[$mom_pos(0) * $mom_sys_lathe_x_factor * $mom_sys_lathe_x_double]
       Z[$mom_pos(2) * $mom_sys_lathe_z_factor]
  }

  BLOCK_TEMPLATE rewind_stop_code
  {
       Text[%]
  }

  BLOCK_TEMPLATE select_main_spindle1_code
  {
       M[303]
  }

  BLOCK_TEMPLATE sequence_number
  {
       N[$mom_seqnum]
  }

  BLOCK_TEMPLATE spindle_css
  {
       G_spin[$mom_sys_spindle_mode_code(SFM)]
       S[$mom_spindle_speed]
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
  }

  BLOCK_TEMPLATE spindle_max_rpm
  {
       G[$mom_sys_reset_code]
       S[$mom_spindle_maximum_rpm]
  }

  BLOCK_TEMPLATE spindle_off
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
  }

  BLOCK_TEMPLATE spindle_range
  {
       M_range[$mom_sys_spindle_range_code($mom_spindle_range)]\opt
  }

  BLOCK_TEMPLATE spindle_rpm
  {
       G_spin[$mom_sys_spindle_mode_code(RPM)]
       S[$mom_spindle_speed]
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
  }

  BLOCK_TEMPLATE spindle_rpm_preset
  {
       G_spin[$mom_sys_spindle_mode_code(RPM)]
       S[$mom_spindle_rpm]
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
  }

  BLOCK_TEMPLATE stop
  {
       M[$mom_sys_program_stop_code]
  }

  BLOCK_TEMPLATE tap
  {
       tap_cycle_M329[329]
       S[$mom_spindle_speed]
  }

  BLOCK_TEMPLATE thread_move
  {
       G_motion[$mom_sys_lathe_thread_advance_type($mom_lathe_thread_advance_type)]
       X[$mom_pos(0) * $mom_sys_lathe_x_factor * $mom_sys_lathe_x_double]
       Z[$mom_pos(2) * $mom_sys_lathe_z_factor]
       thread_lead[\$mom_lathe_thread_lead]
  }

  BLOCK_TEMPLATE tool_change_axis_preset
  {
       G[$mom_sys_reset_code]
       X[$mom_from_ref_pos(0)]
       Z[$mom_from_ref_pos(2)]
  }

  BLOCK_TEMPLATE tool_len_adj_off
  {
       H[$mom_sys_zero]
  }

  BLOCK_TEMPLATE tool_length_adjust
  {
       H[$mom_tool_adjust_register]
  }

  BLOCK_TEMPLATE tool_preselect
  {
       T[$mom_next_tool_number]
  }

  BLOCK_TEMPLATE post_cycle_set
  {
       G_motion[$mom_sys_cycle_reps_code]
       X[$mom_cycle_feed_to_pos(0)]
       Z[$mom_cycle_feed_to_pos(2)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]\opt
       F[$feed]
  }

  BLOCK_TEMPLATE comment_data
  {
       LF_XABS[$mom_pos(0)]
       LF_ZABS[$mom_pos(2)]
       LF_FEED[$mom_feed_rate]
       LF_SPEED[$mom_spindle_speed]
  }

}
