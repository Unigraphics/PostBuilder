MACHINE  mill

INCLUDE {
         $UGII_CAM_USER_DEF_EVENT_DIR/ude.cdl
        }

FORMATTING
{
  WORD_SEPARATOR " "
  END_OF_LINE ""
  SEQUENCE sequence_number 10  10 1 9999

################ FORMAT DECLARATIONS #################
  FORMAT AbsCoord "&__5.4_"
  FORMAT Coordinate "&__4.4_"
  FORMAT Digit_2 "%s"
  FORMAT Digit_3 "&__3_00"
  FORMAT Digit_4 "&_04_00"
  FORMAT Digit_5 "&__5_00"
  FORMAT Dwell_SECONDS "&__5.3_"
  FORMAT Dwell_sec "&__5.3_"
  FORMAT EventNum "&+03_00"
  FORMAT Feed "&__7.2_"
  FORMAT Feed_DPM "&__5.2_"
  FORMAT Feed_FRN "&__5.3_"
  FORMAT Feed_INV "&__5.3_"
  FORMAT Feed_IPM "&__4.1_"
  FORMAT Feed_IPR "&__1.4_"
  FORMAT Feed_MMPM "&__5.0_"
  FORMAT Feed_MMPR "&__2.3_"
  FORMAT Hcode "&_02_00"
  FORMAT Rev "&__4_00"
  FORMAT Rotary "&__5.3_"
  FORMAT String "%s"
  FORMAT Tcode "&__2_00"
  FORMAT Zero_int "&_01_0_"
  FORMAT Zero_real "&_01.10"

################ ADDRESS DECLARATIONS ################
  ADDRESS I
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "X"
      ZERO_FORMAT Zero_real
  }

  ADDRESS G_plane
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_adjust
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_feed
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_spin
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_return
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G_motion
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         9999 Truncate
      MIN         -9999 Truncate
      LEADER      ""
  }

  ADDRESS G_mode
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "G"
  }

  ADDRESS G
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "G"
  }

  ADDRESS X
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(X)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Y
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(Y)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS Z
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(Z)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS fourth_axis
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(fourth_axis)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS fifth_axis
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      [$mom_sys_leader(fifth_axis)]
      ZERO_FORMAT Zero_real
  }

  ADDRESS J
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Y"
      ZERO_FORMAT Zero_real
  }

  ADDRESS K
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Z"
      ZERO_FORMAT Zero_real
  }

  ADDRESS R
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "R"
      ZERO_FORMAT Zero_real
  }

  ADDRESS M128
  {
      FORMAT      AbsCoord
      FORCE       off
      LEADER      "M"
      ZERO_FORMAT Zero_real
  }

  ADDRESS F
  {
      FORMAT      Feed
      FORCE       off
      MAX         999999.9999 Truncate
      MIN         0.0001 Truncate
      LEADER      "F"
      ZERO_FORMAT Zero_real
  }

  ADDRESS FMAX
  {
      FORMAT      String
      FORCE       off
      LEADER      "F"
  }

  ADDRESS T
  {
      FORMAT      Tcode
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      " "
      ZERO_FORMAT Zero_int
  }

  ADDRESS S
  {
      FORMAT      Digit_5
      FORCE       off
      MAX         99999 Truncate
      MIN         0 Truncate
      LEADER      "S"
      ZERO_FORMAT Zero_int
  }

  ADDRESS D
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "D"
  }

  ADDRESS H
  {
      FORMAT      Hcode
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "H"
  }

  ADDRESS dwell
  {
      FORMAT      Dwell_sec
      FORCE       always
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_dwell
  {
      FORMAT      Dwell_sec
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "Q211="
      ZERO_FORMAT Zero_real
  }

  ADDRESS P_cutcom
  {
      FORMAT      Dwell_sec
      FORCE       off
      MAX         99999.999 Truncate
      MIN         0.001 Truncate
      LEADER      "P"
      ZERO_FORMAT Zero_real
  }

  ADDRESS K_cycle
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "K"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_step1
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "I"
      ZERO_FORMAT Zero_real
  }

  ADDRESS cycle_step
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS Q_cutcom
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "Q"
      ZERO_FORMAT Zero_real
  }

  ADDRESS E
  {
      FORMAT      Coordinate
      FORCE       off
      MAX         9999.9999 Truncate
      MIN         -9999.9999 Truncate
      LEADER      "E"
      ZERO_FORMAT Zero_real
  }

  ADDRESS M_spindle
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
  }

  ADDRESS M_range
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
  }

  ADDRESS M_coolant
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         99 Truncate
      MIN         0 Truncate
      LEADER      "M"
  }

  ADDRESS M
  {
      FORMAT      Digit_2
      FORCE       off
      LEADER      "M"
  }

  ADDRESS circle_direction
  {
      FORMAT      String
      LEADER      ""
  }

  ADDRESS cutter_comp
  {
      FORMAT      String
      FORCE       off
      LEADER      ""
  }

  ADDRESS G_cutcom
  {
      FORMAT      Digit_2
      FORCE       off
      MAX         999999999.9999 Truncate
      MIN         -999999999.9999 Truncate
      LEADER      ""
  }

  ADDRESS TL
  {
      FORMAT      Coordinate
      FORCE       off
      LEADER      "L"
      ZERO_FORMAT Zero_real
  }

  ADDRESS TR
  {
      FORMAT      Coordinate
      FORCE       off
      LEADER      "R"
      ZERO_FORMAT Zero_real
  }

  ADDRESS N
  {
      FORMAT      Digit_4
      FORCE       off
      MAX         9999 Truncate
      LEADER      [$mom_sys_leader(N)]
  }

  ADDRESS Text
  {
      FORMAT      String
      FORCE       always
      LEADER      ""
  }


################ ADDRESS DECLARATIONS ################
  ADDRESS LF_AAXIS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_ENUM
  {
      FORMAT      Digit_5
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_BAXIS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_XABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_FEED
  {
      FORMAT      Feed
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_YABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }

  ADDRESS LF_SPEED
  {
      FORMAT      Rev
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_int
  }

  ADDRESS LF_ZABS
  {
      FORMAT      AbsCoord
      FORCE       always
      LEADER      ""
      ZERO_FORMAT Zero_real
  }


############ BLOCK TEMPLATE DECLARATIONS #############
  BLOCK_TEMPLATE auxfun
  {
       M[$mom_auxfun]
  }

  BLOCK_TEMPLATE blank_form_1
  {
       Text[BLK FORM 0.1 Z]
       X[0]
       Y[0]
       Z[-20]
  }

  BLOCK_TEMPLATE blank_form_2
  {
       Text[BLK FORM 0.2]
       X[100]
       Y[100]
       Z[0]
  }

  BLOCK_TEMPLATE circular_move
  {
       Text[CC]
       I[$mom_pos_arc_center(0)]
       J[$mom_pos_arc_center(1)]
       K[$mom_pos_arc_center(2)]
  }

  BLOCK_TEMPLATE circular_move_1
  {
       G_motion[C]\opt
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       F[$feed]
       circle_direction[$mom_sys_circle_code($mom_arc_direction)]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE coolant_off
  {
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE cutcom_off
  {
       cutter_comp[R0]
  }

  BLOCK_TEMPLATE cycl_def_19_0
  {
       Text[CYCL DEF 19.0 WORKING PLANE]
  }

  BLOCK_TEMPLATE cycl_def_19_1
  {
       Text[CYCL DEF 19.1]
       fourth_axis[$mom_out_angle_pos(0)]
       fifth_axis[$mom_out_angle_pos(1)]
  }

  BLOCK_TEMPLATE cycl_def_19_1_null
  {
       Text[CYCL DEF 19.1]
  }

  BLOCK_TEMPLATE cycle_block_dwell
  {
       cycle_dwell[$mom_cycle_delay]
  }

  BLOCK_TEMPLATE cycle_parameters
  {
       G_motion[$mom_sys_linear_code]
       fourth_axis[$mom_out_angle_pos(0)]
       fifth_axis[$mom_out_angle_pos(1)]
       F[$feed]
  }

  BLOCK_TEMPLATE cycle_parameters_1
  {
       G_motion[$mom_sys_linear_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       Text[R0 F MAX]
  }

  BLOCK_TEMPLATE cycle_rapidtoZ
  {
       G_motion[$mom_sys_rapid_code]
       Z[$mom_cycle_rapid_to_pos(2)]
       Text[F MAX]
       cutter_comp[$cutter_comp]\opt
  }

  BLOCK_TEMPLATE delay
  {
       G[$mom_sys_delay_code($mom_delay_mode)]
       dwell[$mom_delay_value]
  }

  BLOCK_TEMPLATE end_of_program
  {
       M[$mom_sys_rewind_code]
  }

  BLOCK_TEMPLATE end_of_program_1
  {
       Text[END PGM 100 $mom_user_output_unit]
  }

  BLOCK_TEMPLATE fifth_axis_rotate_move
  {
       G_motion[$mom_sys_rapid_code]
       fifth_axis[$mom_out_angle_pos(1)]
  }

  BLOCK_TEMPLATE fourth_axis_rotate_move
  {
       G_motion[$mom_sys_rapid_code]
       fourth_axis[$mom_out_angle_pos(0)]
  }

  BLOCK_TEMPLATE helix_pos
  {
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
  }

  BLOCK_TEMPLATE initial_move
  {
       Text[TOOL DEF ]
       T[$mom_tool_number]
       TL[\$mom_tool_length]
       TR[\$mom_tool_radius]
  }

  BLOCK_TEMPLATE initial_move_1
  {
       M[129]
  }

  BLOCK_TEMPLATE initial_move_2
  {
       G_motion[$mom_sys_rapid_code]
       M[126]
  }

  BLOCK_TEMPLATE initial_move_3
  {
       G_motion[$mom_sys_rapid_code]
       M[117]
  }

  BLOCK_TEMPLATE initial_move_4
  {
       G_motion[$mom_sys_rapid_code]
       M[128]
  }

  BLOCK_TEMPLATE initial_move_5
  {
       Text[L M116]
  }

  BLOCK_TEMPLATE linear_move
  {
       G_motion[$mom_sys_linear_code]
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       fourth_axis[$mom_out_angle_pos(0)]
       fifth_axis[$mom_out_angle_pos(1)]
       F[$feed]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE manual_tool_change
  {
       M[$mom_sys_program_stop_code]
  }

  BLOCK_TEMPLATE opstop
  {
       M[$mom_sys_optional_stop_code]\opt\nows
  }

  BLOCK_TEMPLATE output_m128
  {
       G_motion[$mom_sys_rapid_code]
       M128[128]
       F[$feed]
  }

  BLOCK_TEMPLATE output_unit
  {
       G[$mom_sys_unit_code($mom_output_unit)]
  }

  BLOCK_TEMPLATE post_cycle_set
  {
       G_plane[$mom_sys_cutcom_plane_code($mom_cutcom_plane)]\opt
       G_motion[$mom_sys_cycle_reps_code]
       X[$mom_cycle_feed_to_pos(0)]
       Y[$mom_cycle_feed_to_pos(1)]
       Z[$mom_cycle_feed_to_pos(2)]
       fourth_axis[$mom_out_angle_pos(0)]
       fifth_axis[$mom_out_angle_pos(1)]
       R[$mom_cycle_rapid_to_pos($mom_cycle_spindle_axis)]
       F[$feed]
  }

  BLOCK_TEMPLATE prefun
  {
       G[$mom_prefun]
  }

  BLOCK_TEMPLATE rapid_rotary
  {
       G_motion[$mom_sys_rapid_code]
       fourth_axis[$mom_out_angle_pos(0)]
       fifth_axis[$mom_out_angle_pos(1)]
       FMAX[MAX]
  }

  BLOCK_TEMPLATE rapid_spindle
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       FMAX[MAX]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE rapid_spindle_x 
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_pos(0)]
       FMAX[MAX]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE rapid_spindle_y 
  {
       G_motion[$mom_sys_rapid_code]
       Y[$mom_pos(1)]
       FMAX[MAX]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE rapid_spindle_z 
  {
       G_motion[$mom_sys_rapid_code]
       Z[$mom_pos(2)]
       FMAX[MAX]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE rapid_traverse
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       FMAX[MAX]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE rapid_traverse_xy 
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_pos(0)]
       Y[$mom_pos(1)]
       FMAX[MAX]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE rapid_traverse_yz 
  {
       G_motion[$mom_sys_rapid_code]
       Y[$mom_pos(1)]
       Z[$mom_pos(2)]
       FMAX[MAX]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE rapid_traverse_xz 
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_pos(0)]
       Z[$mom_pos(2)]
       FMAX[MAX]
       G_cutcom[$mom_sys_cutcom_code($mom_cutcom_status)]\opt
  }

  BLOCK_TEMPLATE return_home_bc
  {
       G_motion[$mom_sys_rapid_code]
       fourth_axis[0]
       fifth_axis[0]
       FMAX[MAX]
  }

  BLOCK_TEMPLATE return_home_xy
  {
       G_motion[$mom_sys_rapid_code]
       X[$mom_sys_home_pos(0)]
       Y[$mom_sys_home_pos(1)]
       FMAX[MAX]
       M[91]
  }

  BLOCK_TEMPLATE return_home_z
  {
       G_motion[$mom_sys_rapid_code]
       Z[$mom_sys_home_pos(2)]
       fourth_axis[0]
       fifth_axis[0]
       FMAX[MAX]
       M[91]
  }

  BLOCK_TEMPLATE return_motion
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE return_motion_1
  {
       G_mode[91]
       Z[0]
       T[$mom_tool_number]
       H[$mom_sys_zero]
  }

  BLOCK_TEMPLATE return_motion_2
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
       M_coolant[$mom_sys_coolant_code(OFF)]
  }

  BLOCK_TEMPLATE return_motion_5
  {
       G_mode[91]
       Z[0]
       H[$mom_sys_zero]
  }

  BLOCK_TEMPLATE rewind_stop_code
  {
       Text[BEGIN PGM $mom_program_name]
  }

  BLOCK_TEMPLATE sequence_number
  {
       N[$mom_seqnum]
  }

  BLOCK_TEMPLATE spindle_off
  {
       M_spindle[$mom_sys_spindle_direction_code(OFF)]
  }

  BLOCK_TEMPLATE spindle_on
  {
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
       M_coolant[$mom_sys_coolant_code(ON)]
  }

  BLOCK_TEMPLATE spindle_rpm
  {
       S[$mom_spindle_speed]
       M_spindle[$mom_sys_spindle_direction_code($mom_spindle_direction)]\opt
  }

  BLOCK_TEMPLATE spline_start
  {
       Text[L]
       X[$mom_nurbs_points(0,0)]
       Y[$mom_nurbs_points(0,1)]
       Z[$mom_nurbs_points(0,2)]
  }

  BLOCK_TEMPLATE start_of_program
  {
       Text[0 BEGIN PGM 100 $mom_user_output_unit]
  }

  BLOCK_TEMPLATE stop
  {
       M[$mom_sys_program_stop_code]
  }

  BLOCK_TEMPLATE thread_mill
  {
       G_motion[$mom_sys_linear_code]
       fourth_axis[$mom_out_angle_pos(0)]
       fifth_axis[$mom_out_angle_pos(1)]
       F[$feed]
  }

  BLOCK_TEMPLATE thread_mill_1
  {
       G_motion[$mom_sys_linear_code]
       X[$mom_cycle_rapid_to_pos(0)]
       Y[$mom_cycle_rapid_to_pos(1)]
       Text[R0 F MAX]
  }

  BLOCK_TEMPLATE tool_change
  {
       G_mode[$mom_sys_output_code(INCREMENTAL)]
       G[$mom_sys_return_code]
       Z[$mom_sys_home_pos(2)]
  }

  BLOCK_TEMPLATE tool_change_1
  {
       Text[TOOL CALL]
       T[$mom_tool_number]
       Text[Z]
       S[$mom_spindle_speed]
  }

  BLOCK_TEMPLATE tool_length_adjust
  {
       H[$mom_tool_adjust_register]
  }

  BLOCK_TEMPLATE tool_preselect
  {
       T[$mom_next_tool_number]
  }

  BLOCK_TEMPLATE comment_data
  {
       LF_XABS[$mom_pos(0)]
       LF_YABS[$mom_pos(1)]
       LF_ZABS[$mom_pos(2)]
       LF_AAXIS[$mom_pos(3)]
       LF_BAXIS[$mom_pos(4)]
       LF_FEED[$mom_feed_rate]
       LF_SPEED[$mom_spindle_speed]
  }

}
